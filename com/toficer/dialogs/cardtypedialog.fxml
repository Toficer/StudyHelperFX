<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<DialogPane id="cardTypeWindow" fx:controller="com.toficer.dialogs.CardTypeDialogController" xmlns:fx="http://javafx.com/fxml" prefWidth="400">
    <content>
        <GridPane hgap="10" vgap="10" alignment="CENTER">
            <HBox GridPane.rowIndex="0" GridPane.columnIndex="0" alignment="CENTER_RIGHT">
                <Label text="CART TYPE:"/>
            </HBox>
            <ChoiceBox fx:id="cardTypeSelector" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
        </GridPane>
    </content>
</DialogPane>
